{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.6.1",
    "@medusajs/cli": "^2.6.1",
    "@medusajs/framework": "^2.6.1",
    "@medusajs/medusa": "^2.6.1",
    "@mikro-orm/core": "6.4.11",
    "@mikro-orm/knex": "6.4.11",
    "@mikro-orm/migrations": "6.4.11",
    "@mikro-orm/postgresql": "6.4.11",
    "@tanstack/react-query": "^5.69.0",
    "awilix": "^12.0.5",
    "lodash": "^4.17.21",
    "pg": "^8.14.1",
    "react-dropzone": "^14.3.8",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@medusajs/test-utils": "^2.6.1",
    "@mikro-orm/cli": "6.4.11",
    "@swc/core": "1.11.11",
    "@swc/jest": "^0.2.37",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.7.0"
}
